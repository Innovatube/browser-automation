{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://beta.flowzone.cloud/login"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "email"
      },
      "text": "${myEmail}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "password"
      },
      "text": "${myPassword}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.flow-btn"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}